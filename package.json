{
  "name": "@tallman5/cut-sheet-generator",
  "version": "1.0.0-beta.2",
  "description": "An optimized cut sheet generator that efficiently arranges pieces on stock materials, minimizing waste and generating visual layouts in SVG and PNG formats.",
  "author": "tallman5",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tallman5/cut-sheet-generator"
  },
  "homepage": "https://github.com/tallman5/cut-sheet-generator#readme",
  "bugs": {
    "url": "https://github.com/tallman5/cut-sheet-generator/issues"
  },
  "keywords": [
    "woodworking",
    "cut-sheet-generator",
    "cut-list-optimizer",
    "cutting-planner",
    "panel-cutting",
    "bin-packing",
    "guillotine-cutting",
    "kerf-adjustment",
    "material-optimization"
  ],
  "license": "CC-BY-NC-SA-4.0",
  "main": "index.js",
  "files": [
    "/dist",
    "!/dist/**/*.js.map",
    "!/dist/**/*.tsbuildinfo"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "buildpackpub": "tsc && npm pack && npm publish --access=public",
    "coverage": "vitest --coverage",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.13.12",
    "@vitest/coverage-v8": "^3.0.9",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@tallman5/core-js": "^1.0.13"
  }
}
